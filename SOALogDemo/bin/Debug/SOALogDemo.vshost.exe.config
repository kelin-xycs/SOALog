<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <add key="SOALog.Appenders" value="Console, WinForm, File"/>
    <add key="SOALog.File.File" value="aa.log"/>
    <add key="SOALog.Table" value="SOALogTable"/>  
    <add key="SOALog.Columns" value="platform; action; try; exception, create_date, thread_id"/>
    <add key="SOALog.ConnectionName" value="DefaultConnection"/>
  </appSettings>
</configuration>